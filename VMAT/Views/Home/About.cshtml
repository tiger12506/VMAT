@model IEnumerable<VMAT.Models.Organization>

@{
    ViewBag.Title = "About";
}

<div class="padded">
    <h2>Created By</h2>
    <ol class="created-by">
        @foreach (var org in Model)
        {
            <li>
                <h3>@org.Name</h3>
                <ul>
                    @foreach (var author in org.Authors)
                    {
                        <li>@author</li>
                    }
                </ul>
            </li>
        }
    </ol>
</div>
